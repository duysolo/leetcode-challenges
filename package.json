{
  "name": "leetcode-challenges",
  "version": "1.0.0",
  "description": "LeetCode Challenges",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duysolo/leetcode-challenges.git"
  },
  "author": "duysolo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/duysolo/leetcode-challenges/issues"
  },
  "homepage": "https://github.com/duysolo/leetcode-challenges#readme",
  "scripts": {
    "format": "prettier --write ./src",
    "lint": "gts lint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.8.2",
    "eslint": "^8.57.0",
    "gts": "^5.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,ejs}": [
      "npm run format",
      "npm run lint",
      "git add"
    ],
    "*.{html,css,less,scss}": [
      "npm run format",
      "git add"
    ]
  }
}
